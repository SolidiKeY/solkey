\sorts{
   Struct;
   Memory;
   
   \generic alpha;
}

\functions{
    alpha alpha::selectSt(Struct, Field);
    Struct storeSt(Struct, Field, any);    
}

\rules {

    selectOnStruct {
         \schemaVar \term Struct st;
         \schemaVar \term Field fld;
         \schemaVar \term alpha val;
         \find(alpha::selectSt(storeSt(st, fld, val), fld))
         \replacewith(val)
         \heuristics(simplify)
    };

}